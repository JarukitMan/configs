enable_audio_bell no

# BEGIN_KITTY_THEME
# One Half Light
include current-theme.conf
# END_KITTY_THEME

# BEGIN_KITTY_FONTS
font_family      family="Courier Prime"
bold_font        auto
italic_font      auto
bold_italic_font auto
font_size 10.0
# END_KITTY_FONTS

background_opacity 0.9
confirm_os_window_close 0

shell_integration no-cursor
